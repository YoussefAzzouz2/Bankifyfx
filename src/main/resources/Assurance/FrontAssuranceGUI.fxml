<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Assurance.FrontAssuranceController">
   <children>
      <ImageView fx:id="frontimg" fitHeight="609.0" fitWidth="928.0" layoutX="-4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bb.jpg" />
         </image>
      </ImageView>
       <TableView fx:id="assuranceTable" layoutX="11.0" layoutY="56.0" prefHeight="458.0" prefWidth="455.0" styleClass="custom-table-view" stylesheets="@styles.css">

       <columns>
               <TableColumn fx:id="idAssuranceCol" prefWidth="75.0" text="ID Assurance" />
               <TableColumn fx:id="typeAssuranceCol" prefWidth="100.0" text="Type Assurance" />
               <TableColumn fx:id="montantPrimeCol" prefWidth="100.0" text="Montant Prime" />
               <TableColumn fx:id="nomAssureCol" prefWidth="100.0" text="Nom Assure" />
               <TableColumn fx:id="nomBeneficiaireCol" prefWidth="100.0" text="Nom Beneficiaire" />
               <TableColumn fx:id="infoAssuranceCol" prefWidth="126.0" text="Info Assurance" />
           </columns>
       </TableView>
      <Button layoutX="229.0" layoutY="529.0" mnemonicParsing="false" onAction="#goToFrontAgenceButtonClicked" prefHeight="28.0" prefWidth="106.0" styleClass="goFront-button" stylesheets="@styles.css" text="Nos Agences" />
      <Button layoutX="358.0" layoutY="529.0" mnemonicParsing="false" onAction="#goToFrontCategorieeButtonClicked" prefHeight="28.0" prefWidth="106.0" styleClass="goFront-button" stylesheets="@styles.css" text="Nos Categories" />
      <Label layoutX="14.0" layoutY="14.0" text="Bankify Liste des Assurances" textFill="#140679" underline="true">
         <font>
            <Font name="Copperplate Gothic Bold" size="28.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
